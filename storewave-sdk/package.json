{
  "name": "storewave-sdk",
  "version": "0.1.4",
  "description": "TypeScript SDK for Walrus Storage Marketplace",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "sui",
    "walrus",
    "storage",
    "marketplace",
    "blockchain",
    "typescript",
    "sdk"
  ],
  "author": "Inkray",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.44.0",
    "@mysten/walrus": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "@mysten/walrus": {
      "@mysten/sui": "^1.16.0"
    }
  }
}